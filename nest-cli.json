{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/speculator/src",
  "monorepo": true,
  "root": "apps/cli",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/speculator/tsconfig.app.json"
  },
  "projects": {
    "service-ticker": {
      "type": "application",
      "root": "apps/service-ticker",
      "entryFile": "main",
      "sourceRoot": "apps/service-ticker/src",
      "compilerOptions": {
        "tsConfigPath": "apps/service-ticker/tsconfig.app.json"
      }
    },
    "cli": {
      "type": "application",
      "root": "apps/cli",
      "entryFile": "main",
      "sourceRoot": "apps/cli/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cli/tsconfig.app.json"
      }
    }
  }
}
